    "C_Cpp.clang_format_style": "{BasedOnStyle: LLVM, IndentWidth: 4, ColumnLimit: 0, BreakBeforeBraces: Attach}",
    "$memo": "BasedOnStyle: Mozilla, IndentWidth: 3, ColumnLimit: 0, BreakBeforeBraces: Allman",
    "editor.bracketPairColorization.enabled": true,
